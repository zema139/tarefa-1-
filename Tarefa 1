#include <stdio.h>

int main() {

    int num;
    printf("Digite um número inteiro: ");
    scanf("%d", &num);

    if(num % 5 == 0) {
        printf("O número é múltiplo de 5.\n");
    } else {
        printf("O número não é múltiplo de 5.\n");
    }

